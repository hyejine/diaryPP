<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.example.demo.model.dao.AccountRepository">

  <select id="existsByEmail" resultType="UserDto" >
    SELECT * FROM userinfo WHERE user_email=#{user_email}
    </select>

      <select id="findByEmail" resultType="UserDto" >
    SELECT * FROM userinfo WHERE user_email=#{user_email}
    </select>

          <select id="findById" resultType="UserDto" >
    SELECT * FROM userinfo WHERE id=#{id}
    </select>
  </mapper>